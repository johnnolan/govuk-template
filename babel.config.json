{
    "env": {
        "build": {
            "sourceType": "module",
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "ie": "11",
                            "esmodules": true
                        },
                        "useBuiltIns": "usage",
                        "debug": true,
                        "corejs": 3,
                        "modules": false
                    }
                ]
            ],
            "plugins": [
                [
                    "@babel/plugin-transform-runtime",
                    {
                        "regenerator": true
                    }
                ],
                "@babel/plugin-transform-reserved-words",
                "@babel/plugin-transform-member-expression-literals",
                "@babel/plugin-transform-property-literals"
            ]
        },
        "test": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ]
        }
    }
}